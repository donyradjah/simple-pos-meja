<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codetalenta.meja">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="kPos - Meja"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">


        <activity android:name=".DetailTransaksiActivity"></activity>
        <activity android:name=".DaftarTransaksiActivity" />
        <activity android:name=".KonfirmasiPembelianActivity" />
        <activity android:name=".DaftarMenuActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SettingMejaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>